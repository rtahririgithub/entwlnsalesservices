                <xs:annotation>
                    <xs:documentation>
                        Numeric key for the code based program.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="challengeQuestionList" type="ect:Description" minOccurs="0" maxOccurs="2">
                <xs:annotation>
                    <xs:documentation>
