        <xs:annotation>
          <xs:documentation>Activation or Renewal Offer Header</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="transactionTypeCd" type="xs:string" minOccurs="1">
        <xs:annotation>
          <xs:documentation>Mandatory for offerEligibilityTypeCd = TRANSACTIONAL. Valid values are A - Activation, R- renewal, M - Migration</xs:documentation>
