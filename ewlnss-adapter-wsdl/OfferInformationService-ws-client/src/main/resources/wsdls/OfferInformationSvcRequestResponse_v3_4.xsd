      </xs:element>
      <xs:element name="contractTermId" type="xs:integer" minOccurs="1">
        <xs:annotation>
          <xs:documentation>Mandatory for offerEligibilityTypeCd = TRANSACTIONAL. Term in months: 0, 12, 24, 36</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="productCode" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Transactional product code, such as activation handset SKU</xs:documentation>
        </xs:annotation>
      </xs:element>
