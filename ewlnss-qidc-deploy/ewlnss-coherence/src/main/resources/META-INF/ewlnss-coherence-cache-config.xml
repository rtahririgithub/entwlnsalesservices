<?xml version="1.0" encoding="UTF-8"?>
<cache-config xmlns:xalan="http://xml.apache.org/xalan">
	<!-- CACHING SCHEME MAPPING -->
	<caching-scheme-mapping>
		<cache-mapping>
			<cache-name>ReferencePDSDataService*</cache-name>
			<scheme-name>EWSSReferencePDScheme</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>EntWLNSalesServicesCache.EWSS</cache-name>
			<scheme-name>EWSSDefaultCacheScheme</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>ESDG_SalesContext</cache-name>
			<scheme-name>ESDGSalesContextScheme</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>ESDG_SalesContext_Order</cache-name>
			<scheme-name>SalesContextOrderScheme</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>ESDG_SalesInteraction_Data</cache-name>
			<scheme-name>SalesInteractionDataScheme</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>ESDG_SalesItem_Data</cache-name>
			<scheme-name>SalesItemDataScheme</scheme-name>
		</cache-mapping>
		<cache-mapping>
			<cache-name>*</cache-name>
			<scheme-name>EWSSReferenceGridScheme</scheme-name>
		</cache-mapping>
	</caching-scheme-mapping>
	<caching-schemes>
		<distributed-scheme>
			<scheme-name>EWSSReferenceGridScheme</scheme-name>
			<service-name>EWSSReferenceGridService</service-name>
			<backing-map-scheme>
				<local-scheme>
					<unit-calculator>BINARY</unit-calculator>
					<unit-factor>1048576</unit-factor>
				</local-scheme>
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>
		<distributed-scheme>
			<scheme-name>EWSSReferencePDScheme</scheme-name>
			<service-name>EWSSReferencePDService</service-name>
			<backing-map-scheme>
				<local-scheme>
					<unit-calculator>BINARY</unit-calculator>
					<expiry-delay>24h</expiry-delay>
					<unit-factor>1048576</unit-factor>
					<high-units>1024</high-units>
					<eviction-policy>LRU</eviction-policy>
				</local-scheme>
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>
		<distributed-scheme>
			<scheme-name>ESDGSalesContextScheme</scheme-name>
			<service-name>ESDGSalesContextService</service-name>
			<backing-map-scheme>
				<local-scheme>
					<unit-calculator>BINARY</unit-calculator>
					<expiry-delay>24h</expiry-delay>
					<unit-factor>1048576</unit-factor>
					<high-units>1024</high-units>
					<eviction-policy>LRU</eviction-policy>
				</local-scheme>
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>
		<distributed-scheme>
			<scheme-name>SalesContextOrderScheme</scheme-name>
			<service-name>SalesContextOrderService</service-name>
			<backing-map-scheme>
				<read-write-backing-map-scheme>
					<internal-cache-scheme>
						<local-scheme>
							<unit-calculator>BINARY</unit-calculator>
							<expiry-delay>24h</expiry-delay>
							<unit-factor>1048576</unit-factor>
							<high-units>1024</high-units>
							<eviction-policy>LRU</eviction-policy>
						</local-scheme>
					</internal-cache-scheme>
					<cachestore-scheme>
						<class-scheme>
							<class-name>com.telus.csm.essc.cachestore.EsdgContextOrderCacheStore</class-name>
						</class-scheme>
					</cachestore-scheme>
					<write-delay>1s</write-delay>
					<write-batch-factor>0.5</write-batch-factor>
				</read-write-backing-map-scheme>
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>
		<distributed-scheme>
			<scheme-name>SalesInteractionDataScheme</scheme-name>
			<service-name>SalesInteractionDataService</service-name>
			<backing-map-scheme>
				<read-write-backing-map-scheme>
					<internal-cache-scheme>
						<local-scheme>
							<unit-calculator>BINARY</unit-calculator>
							<expiry-delay>24h</expiry-delay>
						</local-scheme>
					</internal-cache-scheme>
					<cachestore-scheme>
						<class-scheme>
							<class-name>com.telus.csm.essc.cachestore.EsdgInteractionDataCacheStore</class-name>
						</class-scheme>
					</cachestore-scheme>
					<write-delay>1s</write-delay>
				</read-write-backing-map-scheme>
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>
		<distributed-scheme>
			<scheme-name>SalesItemDataScheme</scheme-name>
			<service-name>SalesItemDataService</service-name>
			<backing-map-scheme>
				<read-write-backing-map-scheme>
					<internal-cache-scheme>
						<local-scheme>
							<unit-calculator>BINARY</unit-calculator>
							<expiry-delay>24h</expiry-delay>
						</local-scheme>
					</internal-cache-scheme>
					<cachestore-scheme>
						<class-scheme>
							<class-name>com.telus.csm.essc.cachestore.EsdgSalesItemCacheStore</class-name>
						</class-scheme>
					</cachestore-scheme>
					<write-delay>1s</write-delay>
				</read-write-backing-map-scheme>
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>
		<distributed-scheme>
			<scheme-name>EWSSDefaultCacheScheme</scheme-name>
			<service-name>EWSSDefaultCacheService</service-name>
			<backing-map-scheme>
				<local-scheme>
					<unit-calculator>BINARY</unit-calculator>
					<expiry-delay>2h</expiry-delay>
					<unit-factor>1048576</unit-factor>
					<high-units>1024</high-units>
					<eviction-policy>LRU</eviction-policy>
				</local-scheme>
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>
		<proxy-scheme>
			<thread-count>10</thread-count>
			<acceptor-config>
				<serializer>
					<class-name>com.tangosol.io.DefaultSerializer</class-name>
				</serializer>
				<tcp-acceptor>
					<local-address>
						<address system-property="tangosol.coherence.session.proxy.localhost">localhost</address>
						<port system-property="tangosol.coherence.session.proxy.localport">53804</port>
						<reusable>true</reusable>
					</local-address>
				</tcp-acceptor>
			</acceptor-config>
			<autostart system-property="tangosol.coherence.portal.proxy">false</autostart>
		</proxy-scheme>
	</caching-schemes>
</cache-config>
