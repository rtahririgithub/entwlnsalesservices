					<xs:documentation>original sales order</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricePlanIncludedFeature" minOccurs="0">
				<xs:annotation>
					<xs:documentation>ESS derived value from KB for selected price plan</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="pricePlanCode" type="xs:string" minOccurs="0">
