				</xs:annotation>
			</xs:element>
			<xs:element name="corporateOrderItem" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Corporate order attributes for order fulfillment and memo detail</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="corporateEntity" type="tns:CorporateEntity" minOccurs="0">
