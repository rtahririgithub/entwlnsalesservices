			<xs:element name="municipalityName" type="xs:string" minOccurs="0"/>
			<xs:element name="provinceStateCode" type="xs:string" minOccurs="0"/>
			<xs:element name="postalZipCode" type="xs:string" minOccurs="0"/>
			<xs:element name="countryCode" type="xs:string" minOccurs="0"/>
