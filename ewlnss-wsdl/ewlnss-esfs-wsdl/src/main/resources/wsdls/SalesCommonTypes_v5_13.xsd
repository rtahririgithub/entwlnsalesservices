	<xs:complexType name="PricePlan">
		<xs:sequence>
			<xs:element name="pricePlanCode" type="xs:string"/>
			<xs:element name="pricePlanDescriptionTextList" type="ec:MultilingualCodeDescTextList"/>
			<xs:element name="pricePlanAmount" type="xs:double"/>
			<xs:element name="pricePlanCategoryList" type="tns:PricePlanCategoryList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This will model categories for price plans (i.e. size or shareable)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pricePlanDetailURLList" type="tns:PricePlanDetailURLList" minOccurs="0"/>
			<xs:element name="validTermInMonthsList" type="xs:integer" minOccurs="0" maxOccurs="10">
				<xs:annotation>
					<xs:documentation>Applies to Acquisitions only - 0, 12, 24, 36.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="existingPricePlanIndicator" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates the price plan the subscriber is currently on. Not used for new or add-on activations. If all values are FALSE then the subscribers current plan is not available.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="futureDatedPricePlanIndicator" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates that this price plan is future dated on customer's profile</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="grandfatherPricePlanIndicator" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates that this price is grand fathered</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customizedPricePlanIndicator" type="xs:boolean" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates whether this price plan is customized or not.					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="offerPricePlanIndicator" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicators that it is configured as part of an offer, but might not be specific for a customer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="businessAnywherePricePlanIndicator" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates a speicial price plan for Buiness Anywhere bundle customer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="allowCarryoverIndicator" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether this is a carryover price plan.
true - allow to carry over
false - disallow due to SOC policy conflict</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="allowKeepExistingPlanIndicator" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether customer allows to  keep current price with newly selected offer and equipment.
true - allow to keep current plan
false - disallow to keep current plan</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="mustChooseIndicator" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If set to TRUE, customer has to choose this price plan (Eg. corporate offer)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nonDiscountableIndicator" type="xs:boolean" minOccurs="0"/>
			<xs:element name="includedAllowanceList" type="tns:Usage" minOccurs="0" maxOccurs="9">
				<xs:annotation>
					<xs:documentation>Included allowance for minutes, SMS and data usage</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="callingCircleParameter" type="tns:CallingCircleParameterDetail" minOccurs="0">
				<xs:annotation>
					<xs:documentation>*** Obsolete ***
This element is to be removed in ESS V3+ version.
Price plan level calling circle can be found under included service element</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="seatTypeCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Seat type codes "STRT", "OFFC", "MOBL"</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="includedWirelessBackupInd" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>True for Price Plan with included wireless backup soc.</xs:documentation>
				</xs:annotation>
