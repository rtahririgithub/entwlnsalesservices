	<xs:complexType name="GetSalesOrderHistoryType">
		<xs:annotation>
			<xs:documentation>Either search by an known order id or search by subscriber phone</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="operationHeader" type="esc:OperationHeader"/>
			<xs:element name="searchByOrderId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Used for retrieving specific order</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="salesId" type="xs:string">
							<xs:annotation>
								<xs:documentation>This is sales id to identify original sales activity</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="eTransactionId" type="xs:string">
							<xs:annotation>
								<xs:documentation>The e-transaction ID which is referred by internal/external system for this sales transaction. 
DRL - NCCS ID
RTL - TEA ID
TRL - RMS ID</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="searchBySalesIdList" type="xs:string" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Return orders by sales Id</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="searchBySubscriber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Return order list for a subscriber or a BAN</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="billingAccountNumber" type="xs:string" minOccurs="0"/>
						<xs:element name="phoneNumber" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>This is mandatory if order ID is not defined but optional otherwise.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="serviceRequestStatusIdList" type="xs:string" minOccurs="0" maxOccurs="10">
							<xs:annotation>
								<xs:documentation>Status ID list:
2 - Pending
6 - Reserved
10 - Completed (fully completed for both sales pretask and posttask)
5 - Cancelled
12 - Failed</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="dateRange" minOccurs="0">
							<xs:annotation>
								<xs:documentation>by default search for the last 30 days for BC (i.e. if Order ID is specified in the request but search range is empty).  The current 15 days - (DLR reserve/resume scenario) to be overridden with 30 days if Order ID is present.  If searchRange.startDate is NOT null and endDate is null then use the startDate and current date as endDate.  If searchRange.startDate is NULL and endDate is NOT null then use the endDate - 30 days for startDate.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="startDate" type="xs:dateTime" minOccurs="0">
										<xs:annotation>
											<xs:documentation>order search start date.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="endDate" type="xs:dateTime" minOccurs="0">
										<xs:annotation>
											<xs:documentation>order search end date.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="orderFilter" type="tns:searchOrderFilterType" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="searchByCriteriaList" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Return order list which matches search criteria</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="tns:SearchOrderCriteriaType"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="bundleId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This is the bundle order ID for Business Connect clients. Maps to the actor type id in SRPDS.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
